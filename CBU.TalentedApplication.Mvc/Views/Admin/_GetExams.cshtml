@using CBU.TalentedApplication.Business.Model

@model List<Exam>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th scope="col">CriteriaName</th>
            <th scope="col">BranchId</th>
            <th scope="col">MaxValue</th>
            <th scope="col">MinValue</th>
            <th scope="col">Weight</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {


            <tr>
                <td>@item.ExamName</td>
                <td>@item.BranchId</td>
                <td>@item.MaxValue</td>
                <td>@item.MinValue</td>
                <td>@item.Weight</td>
            </tr>
        }
    </tbody>
</table>
